<div id="image-content">

    <div class="entries-nav">
        <a id="arrow-prev" href="#"><img src="/arrow-prev.svg"/></a>
        <a id="arrow-next" href="#"><img src="/arrow-next.svg"/></a>
    </div>

    <div class="post">
        <time>Apr 1st 2020</time>
        <h2 class="title">{{title}}</h2>
        <h3 class="author">{{author}}</h3>
        <h4 class="subtitle">{{subtitle}}</h4>
    </div>

    <div class="post-content">
        {{#each images}}
        <div class="row img">
        <img src="{{col_id_11}}" />

        <div class="epigraph">
            {{col_id_12}}
        </div>
        </div>
        {{/each}}
    </div>

</div>

</div>