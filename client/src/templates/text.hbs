<div id="text-content">

    <div class="col-left">

        <div class="post">
            <time>Apr 1st 2020</time>
            <h2 class="title">{{title}}</h2>
            <h3 class="author">{{author}}</h3>
            <h4 class="subtitle">{{subtitle}}</h4>
        </div>

        <div class="text">
            {{{body}}}
        </div>

    </div>

    
    <div class="col-right"></div>
        {{#each images}}
        <img src="{{col_id_11}}" />
        <p>{{col_id_12}}</p>
        {{/each}}
    </div>

</div>